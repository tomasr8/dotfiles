{
    "window.zoomLevel": -1,
    "editor.fontSize": 19,
    "[javascript]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.fontFamily": "Consolas, 'Monaco', monospace",
    "editor.insertSpaces": true,
    "workbench.editor.enablePreview": false,
    "workbench.startupEditor": "newUntitledFile",
    "python.pythonPath": "C:\\Program Files\\python\\python.exe",
    "python.jediEnabled": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "terminal.integrated.scrollback": 10000,
    "files.associations": {
        "*.pl": "prolog"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "extensions.ignoreRecommendations": false,
    "vscode_custom_css.imports": [
        "file:///C:/Users/tomas/Desktop/dotfiles/dark.css"
    ],
    "workbench.colorTheme": "Atom One Dark",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "bracketPairColorizer.forceIterationColorCycle": false,
    "bracketPairColorizer.forceUniqueOpeningColor": false,
    "bracketPairColorizer.showHorizontalScopeLine": false,
    "bracketPairColorizer.colorMode": "Independent",
